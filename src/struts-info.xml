<struts>
	<!-- Configure NameSpace for information -->
	<package name="Info" namespace="/info" extends="default-web">
		 <action name="showAbout">
            <result>Info.About</result>
        </action>
        
		<!-- Contact -->
		<action name="showContact">
			<result>Info.Contact</result>
		</action>
		 </package>
</struts>